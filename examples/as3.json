{
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
       "class": "ADC",
       "schemaVersion": "3.0.0",
       "label": "asdb",
       "remark": "Simple HTTP application with RR pool",
       "asdb": {
          "class": "Tenant",
          "A1": {
             "class": "Application",
             "template": "http",
             "serviceMain": {
                "class": "Service_HTTP",
                "virtualAddresses": [
                   "1.2.2.2"
                ],
                "pool": "web_pool"
             },
             "web_pool": {
                "class": "Pool",
                "monitors": [
                   "http"
                ],
                "members": [{
                   "servicePort": 80,
                   "serverAddresses": [
                      "192.0.1.1",
                      "192.0.1.2"
                   ]
                }]
             }
          }
       }
    }
 }